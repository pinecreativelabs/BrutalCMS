	<b3grid type="blocks" dir="row" align-h="evenly" align-v="baseline" contain="container-960"/>
	<bento cellgap="0" columns="8" ar="1" bf="false" mcw="50"/>
	<cssgrid flow="row" gaps="0px 0px" place-content="start" place-items="start"/>
