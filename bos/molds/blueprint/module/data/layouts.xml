<?xml version="1.0" encoding="UTF-8"?>
<layouts>
  <layout id="bpl-1" title="full" method="b3grid" cols="1" rows="1" preset="1col" active="true">
    <desc/>
	<column width="100" height="" x-unit="%" min-width="" max-width="100" min-height="" max-height="" y-unit="vh">block</column>
    <rules xxs="" xs="100" sm="100" md="100" lg="100" xl="100" xxl="100"/>
	<b3grid type="blocks" dir="row" align-h="around" align-v="middle" contain="container"/>
	<bento cellgap="0" columns="8" ar="1" bf="false" mcw="50"/>
	<cssgrid flow="row" gaps="0px 0px" place-content="start" place-items="start"/>
  </layout>
