<?xml version="1.0" encoding="UTF-8"?>
<layouts>
  <layout id="1" title="full" method="b3grid" cols="1" rows="1">
    <column width="100" height="" x-unit="%" min-width="" max-width="100" min-height="" max-height="" y-unit="vh">block</column>
    <rules xxs="" xs="100" sm="100" md="100" lg="100" xl="100" xxl="100"/>
  </layout>
  <layout id="2" title="two-column" method="b3grid" cols="2" rows="1">
    <column width="50" height="" x-unit="%" min-width="" max-width="50" min-height="" max-height="" y-unit="vh">block</column>
    <rules xxs="" xs="100" sm="100" md="50" lg="50" xl="50" xxl="50"/>
  </layout>
  <layout id="3" title="three-column" method="b3grid" cols="3" rows="2">
    <column width="33" height="20" x-unit="%" min-width="" max-width="33" min-height="" max-height="" y-unit="vh">block</column>
    <rules xxs="" xs="100" sm="100" md="100" lg="" xl="" xxl=""/>
  </layout>
  <layout id="4" title="four-column" method="b3grid" cols="4" rows="1">
    <column width="25" height="" x-unit="%" min-width="" max-width="25" min-height="" max-height="" y-unit="vh">block</column>
    <rules xxs="" xs="100" sm="100" md="50" lg="25" xl="25" xxl="25"/>
  </layout>
  <layout id="5" title="quad-pane" method="b3grid" cols="2" rows="2">
    <column width="50" height="50" x-unit="vw" min-width="" max-width="" min-height="" max-height="" y-unit="vh">block</column>
    <rules xxs="" xs="100" sm="100" md="" lg="" xl="" xxl="">hide-on-mobile</rules>
  </layout>
  <layout id="6" title="bootstrap-grid" method="bootstrap" cols="3" rows="15">
    <column width="6" height="" x-unit="em" min-width="1" max-width="12" min-height="" max-height="" y-unit="em">col</column>
    <rules xxs="" xs="" sm="12" md="12" lg="" xl="" xxl="">hide-on-tablet</rules>
  </layout>
  
</layouts>
